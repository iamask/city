{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ecocity",
	"main": "src/index.ts",
	"compatibility_date": "2025-01-08",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"assets": {
		"directory": "./public",
		"binding": "ASSETS"
	},
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "ecocity-db",
			"database_id": "1747be6b-5a04-4d60-8f0d-f9ce8f0ef941"
		}
	],
	"ai": {
		"binding": "AI"
	}
}